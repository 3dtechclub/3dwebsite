                <script>
                    $(document).ready(function(){
                      $('#go').prop('disabled',true);
                      $('#searchClear').css('display','none');
                      
                      $('#textsend').keyup(function(){
                        $('#go').prop('disabled', this.value=="" ? true : false); 
                        $('#searchClear').css('display',this.value=="" ? 'none' : 'block');
