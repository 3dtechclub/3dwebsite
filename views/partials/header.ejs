                      $('#textsend').focusin(function(){
                        $('#go').prop('disabled', this.value=="" ? true : false); 
                        $('#searchClear').css('display',this.value=="" ? 'none' : 'block');
                      });
                      $('#textsend').focusout(function(){
                        $('#go').prop('disabled', this.value=="" ? true : true); 
                        $('#searchClear').css('display',this.value=="" ? 'none' : 'block');
                      });
                      $('#textsend').keypress(function(){
                        $('#go').prop('disabled', this.value=="" ? true : false); 
                        $('#searchClear').css('display',this.value=="" ? 'none' : 'block');
                      });
                      

                    }); 
                    function FocusSearch(){
                      document.getElementById("textsend").focus();
                    }
                    var elem=document.getElementById("textsend");
                    elem.value=searchval();
                    function searchval(){
                    var url_string = window.location.href; //window.location.href
                    var url = new URL(url_string);
                    var c = url.searchParams.get("search");
                    return c;
                    }
