<% if(currentUser&&currentUser.isAdmin==true){ %>
    <div style="position:fixed;right:10%;bottom: 10%;">
      <a data-target="#EditProjectModal" data-toggle="modal"><button class="ui circular large icon button"><i class="fas fa-pen"></i></button></a>
      <a href="/Project/<%= thisProject.id %>/remove"><button  class="ui circular negative large icon button "><i class="fas fa-trash"></i></button  ></a>
    </div>
    <div class="modal fade hide" id="EditProjectModal" tabindex="-1" aria-labelledby="editProjectModalLabel" aria-hidden="true">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
              <h5 class="modal-title" id="editProjectModalLabel">Edit Project</h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>
            <div class="modal-body">
                <form class="form justify-content-center" action="/Project/<%= thisProject.id %>/edit" method="POST">
                    <div class="form-group row justify-content-around">
                        <div class="form-group "><label for="prname" class="col-form-label">Name</label>
                        <input type="text" class="form-control" id="prname" placeholder="The Manhattan Project" name="name" value="<%= thisProject.name %>"></div>
                        <div class="form-group"><label for="category" class="col-form-label">Category</label>
                        <input list="categories" class="form-control"id="category" name="category" placeholder="category" value="<%= thisProject.category %>"> </div>                           
                    <datalist id="categories">
                        <option value="Seminar">Seminar</option>
                        <option value="Quiz">Quiz</option>
                    </datalist>
                    </div>
                    
                    <div class="form-group">
                        <label for="image"class="col-form-label">Image</label>
                        <input type="url" class="form-control" id="image" placeholder="URL" name="image" value="<%= thisProject.image %>">
                        <label for="expired" class="col-form-label">Expired</label>
                        <input type="checkbox" checked="checked" id="expired" name="expired" value="true">
                    </div>
                    <div class="form-group">
                        <label for="description" class="col-form-label">Description</label>
                        <textarea name="description" class="form-control" id="description" cols="30" rows="10">
                            <%= thisProject.description %>
                        </textarea>
                    </div>           
                    <div class="form-group">
                      <label for="images" class="col-form-label">Images</label>
                      <textarea name="images" class="form-control" id="images" cols="30" rows="10" placeholder="Images"><% thisProject.images.forEach(function(image){%>
                        <%= image + " || " %>
                      <%});%> </textarea>
                  </div>         
                    <div class="form-group">
                        <label for="members" class="col-form-label">Team Members</label>
                        <textarea name="members" class="form-control" id="members" cols="30" rows="10" placeholder="Team Members"><%= thisProject.team %></textarea>
                    </div>
                    <div class="form-group">
                      <label for="hone" class="col-form-label">Intro</label>
                      <input type="text" class="form-control" id="hone" name="hone" value="<%= thisProject.hone %>">
                  </div>
                    <div class="form-group">
                        <label for="contact" class="col-form-label">Contact</label>
                        <input type="number" class="form-control" id="contact" name="contact" value="<%= thisProject.contact %>">
                    </div>
            </div>
            <div class="modal-footer">

              <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
              <button type="submit" class="btn btn-primary">Save changes</button>
        </form>
       
    </div>
          </div>
        </div>
      </div>
<% } %>

